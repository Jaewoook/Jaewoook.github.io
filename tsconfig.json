{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "lib": ["DOM", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "strict": true,
        "target": "ESNext"
    },
    "include": [
        "src/**/*",
        "./gatsby-browser.*",
        "./gatsby-node.*",
        "./gatsby-ssr.*",
        "./gatsby-ssr.*",
    ]
}
