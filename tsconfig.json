{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "lib": ["DOM", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "strict": true,
        "target": "ES6",
        "noEmit": true,
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src/**/*", "./gatsby-*"]
}
